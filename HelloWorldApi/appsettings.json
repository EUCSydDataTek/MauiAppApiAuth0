{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "your-auth0-domain",
    "Audience": "your-api-audience"
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [ "your-api-audience" ],
        "Authority": "https://your-auth0-domain",
        "ValidIssuer": "your-auth0-domain"
      }
    }
  }
} 